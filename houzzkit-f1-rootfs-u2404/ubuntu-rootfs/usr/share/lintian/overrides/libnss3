libnss3: shared-library-lacks-version usr/lib/*/libfreeblpriv3.so libfreeblpriv3.so
libnss3: shared-library-lacks-version usr/lib/*/libsoftokn3.so libsoftokn3.so
libnss3: shared-library-lacks-version usr/lib/*/libnssdbm3.so libnssdbm3.so
libnss3: shared-library-lacks-version usr/lib/*/libnssckbi.so libnssckbi.so
